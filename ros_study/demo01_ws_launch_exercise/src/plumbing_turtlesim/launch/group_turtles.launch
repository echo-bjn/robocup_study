<launch>
    <!--group标签：启动两对乌龟GUI和键盘控制节点-->
    <group ns="first">
        <node pkg="turtlesim" type="turtlesim_node" name="my_node" output="screen" />
        <node pkg="turtlesim" type="turtle_teleop_key" name="my_key" output="screen" />
    </group>
    <group ns="second">
        <node pkg="turtlesim" type="turtlesim_node" name="my_node" output="screen" />
        <node pkg="turtlesim" type="turtle_teleop_key" name="my_key" output="screen" />
    </group>
</launch>